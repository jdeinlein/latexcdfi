@Article{Dijk2006,
  author       = {van Dijk, Mathilde},
  title        = {{DISCIPLES} {OF} {THE} {DEEP} {DESERT}: {WINDESHEIM} {BIOGRAPHERS} {AND} {THE} {IMITATION} {OF} {THE} {DESERT} {FATHERS}},
