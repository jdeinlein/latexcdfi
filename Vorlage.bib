@Book{Cain2016,
  author    = {Andrew Cain},
  publisher = {Oxford University Press},
  title     = {The Greek Historia Monachorum in Aegypto},
  year      = {2016},
  address   = {Oxford},
  isbn      = {9780191818042},
  month     = {may},
  abstract  = {Historically, most scholarly debates about the Greek Historia monachorum have concerned the nature of the surviving text itself. Do we possess what is essentially the author’s autograph? Most scholars have supposed that what has come down to us is a mutilated redaction of the autograph, itself a lost primitive Greek original on which Rufinus based the Latin translation he made within a decade of the composition of this alleged Vorlage. This chapter employs philological and source-critical analysis to show that their argument is based on faulty presuppositions and suffers fatally from internally inconsistent logic. It is argued that the Greek text as it has been reconstructed by André-Jean Festugière approximates the anonymous author’s autograph, notwithstanding of course minor and ultimately inconsequential variant readings resulting from scribal errors and interventions which crept into the text over the centuries of its transmission.},
  doi       = {10.1093/acprof:oso/9780198758259.001.0001},
  url       = {https://oxford.universitypressscholarship.com/view/10.1093/acprof:oso/9780198758259.001.0001/acprof-9780198758259-chapter-2},
