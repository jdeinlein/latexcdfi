@Article{Krondorfer2013,
  author       = {Krondorfer, Björn},
  title        = {Ritual Denied and Read as Truth: From Totalizing Sincerity to the Seriousness of Play},
  issn         = {08901112},
  note         = {Publisher: Pamela J. Stewart and Andrew J. Strathern},
  number       = {1},
  pages        = {59--72},
  volume       = {27},
  abstract     = {[Modernity, this paper argues, has not so much abandoned ritual as it has employed rhetorical operations by which it denies the ritual character of its own activities. Following the innovative thesis of Adam Seligman (et al.) that modernity's tendency to embrace the mode of sincerity needs the counterweight of a subjunctive as if mode of ritual, this essay offers conceptual thoughts on the denial of ritual as it relates to modernity's mode of sincerity and its absolutizing truth-claims. In the first part, I turn to modalities of political violence as extreme forms of sincerity, in which a professed anti-ritualistic impulse is at work in the service of legitimation and justification. In the second, and shorter, part, I propose a counter-practice to the rhetorical operations that deny ritual in the name of sincerity. Attentiveness to the creative playfulness of the subjunctive orientation points to a ritual practice that engenders a symbolic reality of seriousness without falling into the trap of absolutizing boundaries.]},
  date         = {2013},
  journaltitle = {Journal of Ritual Studies},
  url          = {http://www.jstor.org/stable/44368865},
  urldate      = {2021-01-26},
}

