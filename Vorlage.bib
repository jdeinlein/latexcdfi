@Article{Saak2006,
  author       = {Saak, Eric Leland},
  title        = {"{EX} {VITA} {PATRUM} {FORMATUR} {VITA} {FRATRUM}": {THE} {APPROPRIATION} {OF} {THE} {DESERT} {FATHERS} {IN} {THE} {AUGUSTINIAN} {MONASTICISM} {OF} {THE} {LATER} {MIDDLE} {AGES}},
  issn         = {1871241X, 18712428},
  note         = {Publisher: Brill},
  number       = {1},
  pages        = {191--228},
  volume       = {86},
  abstract     = {[This article traces the role of the desert fathers in the creation of the late medieval Augustinian Myth. It argues that the major problem facing members of the Order of Hermits of Saint Augustine ({OESA}) was how to appropriate the tradition of the desert fathers and that of Augustine's monasticism for the tradition of the Order. In this light, special attention is given to the Pseudo-Augustinian Sermones ad fratres in eremo and the central importance of John Cassian and Paul of Thebes. Of particular importance are the works of Jordan of Quedlinburg, which shaped the identity of the {OESA} from the mid-fourteenth to the early sixteenth century. The desert fathers provided the model of the eremitical life, and thus Jordan "mythified" the desert fathers as he had Augustine himself. This was not an issue of historical identification, but of mythic creation in an attempt to provide the foundation of the late medieval {OESA}.]},
  date         = {2006},
  journaltitle = {Church History and Religious Culture},
  url          = {http://www.jstor.org/stable/23922517},
  urldate      = {2021-01-26},
}

@Article{Hassan2013,
  author       = {Hassan, Naglaa Saad M. and حسن, نجلاء سعد},
  title        = {The Arab Desert in American Poetic Discourse / الصحراء العربية في الخطاب الشعري الأمريكي},
  issn         = {11108673},
  note         = {Publisher: [Department of English and Comparative Literature, American University in Cairo, Department of English and Comparative Literature, American University in Cairo and American University in Cairo Press]},
  number       = {33},
  pages        = {88--113},
  abstract     = {[Since pre-Islamic times, the Arab desert has been a central setting, theme, and motif in Arabic poetry. But how is the Arab desert viewed in American poetic discourse? Starting with a survey of the position of the desert in general and the Arab desert in particular in Judeo-Christian tradition, this article analyzes some representative poems across different periods in the American canon, concluding with an analysis of selected examples from contemporary American poetry. This close survey shows how the image of the Arab desert is boud up with the context from which it emerges — historical, political, economic, or religious. تقوم هذه المقالة بدراسة الصور المتعددة للصحراء العربية في الخطاب الشعرى الأمريكي موضحة أنها ﺗﺘﺒﻮﺃ مكاناً جوهرياً في هذا الخطاب عبر حقب عديدة. تبدأ المقالة بتتبع الصحراء من زاوية عامة - جغرافية ولغوية ودينية - مع التركيز على عمق مغزاها في التراث الديني، وعلى الأخص في الإنجيل والتوارة، حيث کانت الصحراء مكاناً لنزول الوحي والتقرب إلى الله بمثل ما هي مكان موحش للعقاب الإلهي. وبعد تحلیل عدة قصائد لأبرز الشعراء الكلاسيكيين في التراث الأمريكي، تنتقل المقالة إلى الشعر الأمريكي المعاصر لإبراز كيف أن تمثيل الصحراء يخرج من منظور عملي أكثر منه رمزي، ويعتمد على التجربة الواقعية أكثر من اعتماده على الخيال. وعليه، نجد الصحراء ترتبط بالأحداث السياسیة في فلسطين والعراق بقدر ارتباطها بالتجربة السياحية مع احتفاظها بالرمزية الدينية التي يميل بعض الشعراء المعاصرين إلى إلقاء الضوء عليها .]},
  date         = {2013},
  journaltitle = {Alif: Journal of Comparative Poetics},
  url          = {http://www.jstor.org/stable/24487183},
  urldate      = {2021-01-26},
}

